@using CarDealerProject.Models
@model IEnumerable<CarDealer>

@{
    ViewBag.Title = "Car dealers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">
    @if (TempData["ErrorMessage"] != null)
    {
        @Html.Raw("showMessageBox('" + @TempData["ErrorMessage"] + "');") 
    }
</script>

<div class="leftmenu">
    @{Html.RenderPartial("Search", new CarDealer());}
</div>

<div id="dataContainer" class="content">
    @{Html.RenderPartial("ListDataContainer", @Model);}
</div>

<div class="rightmenu">
    @{
        Html.RenderPartial("Create", new CarDealer());
    }
</div>

<div class="clear"></div>
