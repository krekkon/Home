<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="CarDealerProject" namespace="CarDealerProject.Models">
  <class name="CarDealer" table="CarDealer" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>
    <property name="Name" />
    <property name="Email" />
    <property name="Telephone" />
    <property name="ParkingPlaces" />
    <property name="Country" />
    <property name="City" />
    <property name="ZipCode" />
    <property name="Street" />
    <property name="StreetNumber" />
  </class>
</hibernate-mapping>